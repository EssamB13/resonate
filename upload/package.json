{
  "name": "@resonate/upload",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "jalla build index.js",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "jalla index.js",
    "test": "standard && npm run test-deps",
    "test-browser": "browserify -t [babelify] test/browser/index.js | tape-run --port 5000 --keep-open | tap-format-spec",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i @resonate/tachyons"
  },
  "browser": {
    "assert": "nanoassert"
  },
  "browserslist": [
    "last 1 version",
    ">1%",
    "not dead"
  ],
  "browserify": {
    "transform": [
      [
        "@resonate/envlocalify",
        {
          "global": true
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-preset-env": {
        "stage": 1,
        "features": {
          "nesting-rules": true
        }
      },
      "postcss-custom-media": {}
    }
  },
  "standard": {
    "ignore": [
      "/dist/**/*.js"
    ]
  },
  "license": "gpl3",
  "dependencies": {
    "@resonate/api-factory-generator": "file:../packages/api-factory-generator",
    "@resonate/button": "file:../packages/button",
    "@resonate/icon-element": "file:../packages/icon-element",
    "@resonate/input-element": "file:../packages/input-element",
    "@resonate/link-element": "file:../packages/link-element",
    "@resonate/rangeslider": "file:../packages/rangeslider",
    "@resonate/schemas": "file:../packages/schemas",
    "@resonate/tachyons": "file:../packages/tachyons",
    "@resonate/textarea-element": "file:../packages/textarea-element",
    "@resonate/theme-skins": "file:../packages/theme-skins",
    "babel-polyfill": "^6.26.0",
    "choo": "^7.0.0-0",
    "choo-devtools": "^2.5.1",
    "choo-lazy-view": "^1.0.2",
    "choo-meta": "^2.0.0",
    "choo-service-worker": "^2.4.0",
    "form-serialize": "^0.7.2",
    "is-equal-shallow": "^0.1.3",
    "left-pad": "^1.3.0",
    "localforage": "^1.7.3",
    "nanoanimation": "^2.1.0",
    "nanologger": "^1.3.1",
    "nanostate": "^1.2.1",
    "sortablejs": "^1.10.0-rc3",
    "validate-formdata": "^2.1.0",
    "validator": "^10.11.0"
  }
}
